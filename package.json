{
    "name": "eslint-config-pwn",
    "version": "6.0.1",
    "description": "Opinionated ESLint shareable config",
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "author": "Pwn <hi@pwn.buzz>",
    "devDependencies": {
        "js-yaml": "~3.8.4"
    },
    "peerDependencies": {
        "eslint": ">=3"
    },
    "scripts": {
        "prepublish": "npm run build",
        "build": "npm run build:base && npm run build:style",
        "build:base": "echo module.exports=`js-yaml base.yml` > index.js",
        "build:style": "echo module.exports=`js-yaml style.yml` > style.js",
        "clean": "rm -f *.js"
    },
    "license": "MIT",
    "repository": "pwnn/eslint-config-pwn"
}
